<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1;net8.0;net9.0</TargetFrameworks>
    <RootNamespace>System.Collections.ObjectModel</RootNamespace>
    <AssemblyName>System.Collections.ObjectModel.RangedObservableCollection</AssemblyName>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
    <Version>1.0.1</Version>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.1.0</FileVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Copyright>Copyright Â© Matthew Leibowitz</Copyright>
    <PackageId>RangedObservableCollection</PackageId>
    <PackageProjectUrl>https://github.com/mattleibow/RangedObservableCollection</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/mattleibow/RangedObservableCollection/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/mattleibow/RangedObservableCollection</RepositoryUrl>
    <Description>A small extension to ObservableCollection that allows for multiple items to be added, removed or replaced in a single operation.</Description>
    <PackageTags>ObservableCollection,AddRange,ReplaceRange,RemoveRange,RangedObservableCollection</PackageTags>
    <Authors>Matthew Leibowitz</Authors>
    <Company>Matthew Leibowitz</Company>
  </PropertyGroup>

</Project>
